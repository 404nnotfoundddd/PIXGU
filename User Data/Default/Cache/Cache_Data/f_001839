    <script src="/assets/js/masterCargo.min.js?v=lcHBBkBoyoPs4XX03Rv1iiIWx1Cq5SFnpib-HbMoMxM" type="text/javascript"></script>
    

    <script>
        $(window).on('load', function () {

            $('#itemtrack').change(function () {
                var val = $('select[id=itemtrack] option').filter(':selected').val();
                $.ajax({
                    url: "parca-hareket",
                    data: { "consimentoNumber": $('#itemtrack').attr("data-value"), "itemCount": 1 },
                    type: "Get",
                    success: function (data) {
                        $(".trackBox").html(data);
                    }
                });
            });

            if ($(window).width() < 1200) {

                var _playHtml = '<div class="playDot" />';
                var _autoplayTimeOut = 8000;
                $('.cargoTrackingPost .column').owlCarousel({
                    items: 1,
                    smartSpeed: 450,
                    autoplay: true,
                    autoplayTimeout: 8000,
                    margin: 0,
                    loop: true,
                    mouseDrag: true,
                    nav: false,
                    dots: true,
                    dotsEach: true,
                }).on('translated.owl.carousel', function (event) {
                    playDotPost();
                });

                playDotPost();
                function playDotPost() {
                    $('.cargoTrackingPost .column .playDot').remove();
                    $('.cargoTrackingPost .column .owl-dots .owl-dot.active').append(_playHtml);
                    var circlechangeVal = 0;
                    var circle = $('.cargoTrackingPost .column .playDot').circleProgress({
                        value: 1,
                        size: 18,
                        thickness: 2,
                        startAngle: -Math.PI / 2,
                        fill: {
                            color: '#FF5C00',
                        },
                        emptyFill: 'rgba(255, 92, 0, 0.3)',
                        animation: { duration: _autoplayTimeOut },
                    }).on('circle-animation-progress', function (event, progress) {
                        circlechangeVal = progress;
                    });
                }

            }
        });
    </script>
